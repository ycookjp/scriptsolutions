{
    "reporters": [
        "default",
        ["../node_modules/jest-html-reporter", {
            "pageTitle": "Test Report",
            "outputPath": "./test-reports/test-report.html"
        }]
    ],
    "testResultsProcessor": "../node_modules/jest-html-reporter",
    "collectCoverage": true,
    "collectCoverageFrom": [
        "../**/*.{js,jsx}",
        "!../**/node_modules/**",
        "!../**/__tests__/**"
    ],
    "coverageDirectory": "./coverage"
}
